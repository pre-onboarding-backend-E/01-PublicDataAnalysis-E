{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true, 
    "jest": true
  },
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-unused-vars": "warn",
    "import/prefer-default-export": 0,
    "prettier/prettier": ["error"]
  }
}
